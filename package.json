  "author": "Higor Alves <higorhaalves@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/server.js",
    "start:dev": "ts-node-dev -r tsconfig-paths/register --respawn --clear --transpile-only --ignore-watch node_modules src/server.ts",
